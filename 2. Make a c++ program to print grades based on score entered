#include <iostream>
using namespace std;

int main(){
    int Score;
    cout<<"Enter The Score To Know Your Grade:";
    cin>>Score;
    if (Score >= 90){
        cout<<"Your Grade is : A";
    }
    else if (Score >= 80){
        cout<<"Your Grade is : B";
    }
    else if(Score >=60){
    	cout<<"Your Grade is : C";
	}
	else if(Score >=50){
		cout<<"Your Grade is : D";
	}
	else if(Score <=49){
		cout<<"Sorry, You Have Failed";
	}
	return 0;
}
